    <servlet>
        <servlet-name>ServletClientes</servlet-name>
        <servlet-class>com.controlador.ServletClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAutomplete</servlet-name>
        <servlet-class>com.controlador.ServletAutomplete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEliminarEmpleado</servlet-name>
        <servlet-class>com.controlador.ServletEliminarEmpleado</servlet-class>
