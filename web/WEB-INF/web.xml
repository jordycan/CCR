        <servlet-name>ServletSesion</servlet-name>
        <url-pattern>/ServletSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEmpleado</servlet-name>
        <url-pattern>/ServletEmpleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletClientes</servlet-name>
        <url-pattern>/ServletClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAutomplete</servlet-name>
        <url-pattern>/ServletAutomplete</url-pattern>
